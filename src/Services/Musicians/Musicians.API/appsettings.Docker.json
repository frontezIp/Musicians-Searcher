{
  "MongoDbSettings": {
    "Database": "musiciansDB",
    "Host": "mongo",
    "Port": "27017",
    "User": "root",
    "Password": "password"
  },
  "IdentityServer": {
    "TokenUri": "http://localhost:1000/connect/token",
    "IssuerUri": "http://identity:80"
  },
  "ConsumerConfig": {
    "BootstrapServers": "kafka:9092",
    "ClientId": "Musicians",
    "GroupId": "MusicianIdentityConsumer",
    "AutoOffsetReset": "Earliest",
    "EnableAutoCommit": "true",
    "EnableAutoOffsetStore": "false"
  },
  "ConnectionStrings": {
    "redis": "redis"
  },
  "Kestrel": {
    "Endpoints": {
      "grpc": {
        "Url": "http://musicians:2001",
        "Protocols": "Http2"
      },
      "webApi": {
        "Url": "http://musicians",
        "Protocols": "Http1"
      }
    }
  }
}
